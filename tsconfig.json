{
    "include": [
        "./src/**/*.ts",
        "./src/**/*.tsx",
        "./*.css"
    ],
    "compilerOptions":{
        "jsx": "react",
        "jsxFactory": "h",
        "jsxFragmentFactory": "f",
        "moduleResolution": "node",
        "strictNullChecks": true,
        "allowJs": true,
        "module":"ES6",
        "target":"ES6",
        "strict":false
    }
  }